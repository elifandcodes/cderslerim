C Learning Notes and Exercises
#include <stdio.h>
#include <stdlib.h>


/*
Kullanicin girdigi 3 sayidan en buyugunu bulan program.

*/

int main() {
	int sayi1, sayi2, sayi3;
	printf("Lutfen 3 adet sayi giriniz: \n");
	scanf_s("%d %d %d", &sayi1, &sayi2, &sayi3);

	printf("%d %d %d", sayi1, sayi2, sayi3);

	if (sayi1 > sayi2 && sayi1 > sayi3) {
		printf("\n%d sayisi en buyuk sayidir.", sayi1);
	}if (sayi2 > sayi1 && sayi2 > sayi3) {
		printf("\n%d sayisi en buyuk sayidir.", sayi2);
	}if (sayi3 > sayi1 && sayi3 > sayi2) {
		printf("\n%d sayisi en buyuk sayidir", sayi3);
	}
	else {
		printf("\nGirilen sayilar birbirine esit veya en buyuk sayi yok.");
	}

	return 0;
}
